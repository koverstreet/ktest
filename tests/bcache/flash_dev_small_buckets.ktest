#!/bin/bash

require-lib bcache-test-libs.sh

config-cache 2G
config-bucket-size 8k
config-block-size 2k
config-volume 600M

config-timeout $(($(stress_timeout) * 2))

main()
{
    setup_tracing 'bcache:*'

    setup_bcache
    run_antagonist
    run_bcache_stress
    stop_volumes
    run_bcachefs_stress
    stop_bcache
}
